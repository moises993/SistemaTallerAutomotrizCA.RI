@model tema.Models.Expediente

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    * {
        font-family: Arial;
    }
</style>
<link href="~/Plantilla/dist/css/Tablas.css" rel="stylesheet" />
<div class="content-wrapper">
    <div class="container">

        <ol class="breadcrumb" style="border:solid;color:black;background-color: #01386E">
            <li><a asp-controller="Expediente" asp-action="Index" style="color:aliceblue">Expedientes&nbsp;/&nbsp;</a></li>
            <li><a class="active" style="color:aliceblue">Eliminar expediente</a></li>
            @*<li><a href="#">Home</a></li>
                <li><a href="#">Private</a></li>
                <li><a href="#">Pictures</a></li>
                <li class="active">Vacation</li>*@
        </ol>
        <h1>Eliminar Cliente</h1>

    
        <div>
            <h4>Expediente</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.IDExpediente)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.IDExpediente)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.IDVehiculo)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.IDVehiculo)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.nombreTecnico)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.nombreTecnico)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.asunto)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.asunto)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.descripcion)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.descripcion)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.fechaCreacionExp)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.fechaCreacionExp)
                </dd>
                <dt class="col-sm-2">
