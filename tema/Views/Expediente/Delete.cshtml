                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.nombreCliente)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.marca)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.marca)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.modelo)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.modelo)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.placa)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.placa)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="submit" value="Elimar expediente" class="Boton" /> |
                <a asp-action="Index" class="Boton">Ver lista de expedientes</a>
            </form>
        </div>
    </div>
</div>